{
  "ConnectionStrings": 
  {
    "DefaultConnection": "Server=(local);Database=WebApp;Trusted_Connection=True;MultipleActiveResultSets=true"
  },

  "MongoDbSettings": 
  {
    "DatabaseConnectionString": "mongodb://localhost:27017"
  },


  // LogLevel Severity: "Trace", "Debug", "Information", "Warning", "Error", "Critical", "None"
  "Logging": {
    "PathFormat": "Logs/log-{Date}.log",
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
